{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "unused-imports"
    ],
    "rules": {
        "no-empty": 1,
        "react/react-in-jsx-scope": 1,
        "react/prop-types": 1
    }
}